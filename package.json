{
  "name": "rickandmorty",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build public --prod --aot --build-optimizer --named-chunks",
    "compress": "gzipper --output-file-format [filename].[ext] -i TTF,svg,html,css,js,json,eot,woff,png,woff2,ico,webmanifest,icon --verbose ./dist/rickandmorty ./dist-real",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/http": "^7.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "apollo": "^2.30.1",
    "apollo-angular": "^1.10.0",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
